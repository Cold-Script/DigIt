local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/TheNuggetEater05/ekaMiffutSmodnaR/main/EssentialLib", true))()
local Window = Library:CreateMain({title = "YOUHUB", desc = "by rechedmcvn"})
local Tabs = {
Main = Window:CreateTab({image = "http://www.roblox.com/asset/?id=6026568198", title = "Main", desc = "This section has functions that will help you.", ismain = true})
Players = Window:CreateTab({image = "http://www.roblox.com/asset/?id=6022668945", title = "Player", desc = "This is tab functions about the player."})
}
Tabs.Main:AddToggle({text = "Auto Dig", side = "left", callback = function(v)
_G.AutoDig = v
while _G.AutoDig and wait(1) do
if not game.Players.LocalPlayer.PlayerGui.Main:FindFirstChild("DigMinigame") then
game:GetService("ReplicatedStorage").Source.Network.RemoteFunctions.Digging:InvokeServer({
Command = "CreatePile"
})
end
end
end})
Tabs.Main:AddToggle({text = "No Miss", side = "right", callback = function(v)
_G.NoMiss = v
if _G.NoMiss then
NoMiss = game.Players.LocalPlayer.PlayerGui.Main.ChildAdded:Connect(function()
game.Players.LocalPlayer.PlayerGui.Main.DigMinigame.Arena.Size = UDim2.new(9e9,0,1,0)
end)
else
NoMiss:Disconnect()
end
end})






